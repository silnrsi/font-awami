lookup _InsBlockerAndDecKw {
	# Decrement the KwMarker, because it got incremented before we knew there was going to be blocker further in.
	sub kw0		by	kblock	kw0;
	sub kw100	by	kblock	kw0;
	sub kw200	by	kblock	kw100;
	sub kw300	by	kblock	kw200;
	sub kw400	by	kblock	kw300;
	sub kw500	by	kblock	kw400;
	sub kw600	by	kblock	kw500;
	sub kw700	by	kblock	kw600;
	sub kw800	by	kblock	kw700;
	sub kw900	by	kblock	kw800;
	sub kw1000	by	kblock	kw900;
	sub kw1100	by	kblock	kw1000;
	sub kw1200	by	kblock	kw1100;
	sub kw1300	by	kblock	kw1200;
	sub kw1400	by	kblock	kw1300;
	sub kw1500	by	kblock	kw1400;
	sub kw1600	by	kblock	kw1500;
} _InsBlockerAndDecKw;

# If any blocker is present, insert one right before the KwMarker, which should terminate the kerning calculation process.
lookup _EnforceBlock {
	lookupflag UseMarkFilteringSet [@KwMarker kblock];

	# TODO: figure out how many of these rules we need.
	# I don't think there  can ever be more than one kblock between the KwMarkers, because once we 
	# create the first blocker, the remaining _TryNext lookups can't do anything.

	ignore sub kblock @KwMarker' @KwMarker;
	ignore sub kblock @KwMarker' kblock @KwMarker;
	# ignore sub kblock @KwMarker' kblock kblock @KwMarker;
	# ignore sub kblock @KwMarker' kblock kblock kblock @KwMarker;
	# ignore sub kblock @KwMarker' kblock kblock kblock kblock @KwMarker;
	# ignore sub kblock @KwMarker' kblock kblock kblock kblock kblock @KwMarker;
	# ignore sub kblock @KwMarker' kblock kblock kblock kblock kblock kblock @KwMarker;

	sub @KwMarker' lookup _InsBlockerAndDecKw   kblock @KwMarker;
	# sub @KwMarker' lookup _InsBlockerAndDecKw   kblock kblock @KwMarker;
	# sub @KwMarker' lookup _InsBlockerAndDecKw   kblock kblock kblock @KwMarker;
	# sub @KwMarker' lookup _InsBlockerAndDecKw   kblock kblock kblock kblock @KwMarker;
	# sub @KwMarker' lookup _InsBlockerAndDecKw   kblock kblock kblock kblock kblock @KwMarker;
	# sub @KwMarker' lookup _InsBlockerAndDecKw   kblock kblock kblock kblock kblock kblock @KwMarker;

} _EnforceBlock;


