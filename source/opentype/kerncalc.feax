lookup _DelYtAndIncKw {
    lookupflag UseMarkFilteringSet [@KwMarker @YtMarker];
    sub @YtMarker  kw0		by  kw100;
    sub @YtMarker  kw100	by  kw200;
    sub @YtMarker  kw200	by  kw300;
    sub @YtMarker  kw300	by  kw400;
    sub @YtMarker  kw400	by  kw500;
    sub @YtMarker  kw500	by  kw600;
    sub @YtMarker  kw600	by  kw700;
    sub @YtMarker  kw700	by  kw800;
    sub @YtMarker  kw800	by  kw900;
    sub @YtMarker  kw900	by  kw1000;
    sub @YtMarker  kw1000	by  kw1100;
    sub @YtMarker  kw1100	by  kw1200;
    sub @YtMarker  kw1200	by  kw1300;
    sub @YtMarker  kw1300	by  kw1400;
    sub @YtMarker  kw1400	by  kw1500;
    sub @YtMarker  kw1500	by  kw1600;
    sub @YtMarker  kw1600	by  kw1600;
} _DelYtAndIncKw;

lookup _TryNext16 {
	ignore sub @YbMarker';
	ignore sub @YtMarker' @YbMarker;
} _TryNext16;

# This loop creates a set of lookups called _TryNext2, _TryNext3, ... _TryNext15, each of which calls
# the successive lookup in the set to continue shifting yt markers.

do	forlet ntry = range(15,1,-1);
	let nextTry = ntry + 1;
	let lname = "_TryNext" + str(ntry);
	let lnameNext = "_TryNext" + str(nextTry);
	{
		lookup $lname {
			ignore sub @YbMarker';				# blocked, or nothing left to shift
			ignore sub @YtMarker' @YbMarker;	# nothing left to shift
			sub @YtMarker' yt0'   lookup _Block lookup _DelYt	lookup _InsYt0		@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt100' lookup _Block lookup _DelYt	lookup _InsYt100	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt200' lookup _Block lookup _DelYt	lookup _InsYt200	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt300' lookup _Block lookup _DelYt	lookup _InsYt300	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt400' lookup _Block lookup _DelYt	lookup _InsYt400	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt500' lookup _Block lookup _DelYt	lookup _InsYt500	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt600' lookup _Block lookup _DelYt	lookup _InsYt600	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt700' lookup _Block lookup _DelYt	lookup _InsYt700	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt800' lookup _Block lookup _DelYt	lookup _InsYt800	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt900' lookup _Block lookup _DelYt	lookup _InsYt900	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1000' lookup _Block lookup _DelYt	lookup _InsYt1000	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1100' lookup _Block lookup _DelYt	lookup _InsYt1100	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1200' lookup _Block lookup _DelYt	lookup _InsYt1200	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1300' lookup _Block lookup _DelYt	lookup _InsYt1300	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1400' lookup _Block lookup _DelYt	lookup _InsYt1400	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1500' lookup _Block lookup _DelYt	lookup _InsYt1500	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1600' lookup _Block lookup _DelYt	lookup _InsYt1600	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1700' lookup _Block lookup _DelYt	lookup _InsYt1700	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1800' lookup _Block lookup _DelYt	lookup _InsYt1800	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt1900' lookup _Block lookup _DelYt	lookup _InsYt1900	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt2000' lookup _Block lookup _DelYt	lookup _InsYt2000	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt2100' lookup _Block lookup _DelYt	lookup _InsYt2100	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt2200' lookup _Block lookup _DelYt	lookup _InsYt2200	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt2300' lookup _Block lookup _DelYt	lookup _InsYt2300	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt2400' lookup _Block lookup _DelYt	lookup _InsYt2400	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt2500' lookup _Block lookup _DelYt	lookup _InsYt2500	@YbMarker' lookup $lnameNext;
			sub @YtMarker' yt2600' lookup _Block lookup _DelYt	lookup _InsYt2600	@YbMarker' lookup $lnameNext;
